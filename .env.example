TZ=UTC
PORT=3333
HOST=localhost
LOG_LEVEL=info
APP_KEY=
NODE_ENV=development
SESSION_DRIVER=cookie
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=

# Weather API Configuration
# Get your API key from: https://openweathermap.org/api
OPENWEATHER_API_KEY=your_openweathermap_api_key_here

# Alternative weather API - Get your API key from: https://www.weatherapi.com/
WEATHER_API_KEY=your_weatherapi_key_here

# Optional: IP location API key (many services work without a key)
IPAPI_KEY=your_ipapi_key_here

# Weather API preference (openweather or weatherapi)
WEATHER_DEFAULT_API=openweather
